<Window x:Class="MC_Buttery_Launcher.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MC_Buttery_Launcher"
        mc:Ignorable="d"
        Title="Settings - Minecraft Launcher" Height="90" Width="230"
        Closed="OnClose" MinWidth="230" MinHeight="90">
    <Grid>
        <CheckBox x:Name="KeepOpenCheckBox" Content="Keep Open" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
